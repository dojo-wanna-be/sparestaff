<div class="admin_sub_header">
  <div>
    <%= link_to 'Backend Home', '', class: 'sub_link' %>
    &nbsp;&gt; Users
  </div>
</div>

<div class="admin_panle_body">
  <%= render partial: "layouts/admin/left_hand_navigation" %>
  <div class="admin_right_panel">
    <div class="step_heading">Hirings</div>
    <div class="right_step user">
    <%= form_tag(search_admin_hirings_path, method: :get, id: 'search_form', remote: true)do %>
      <%= render partial: "hiring_search_bar"%>
    <% end %>
      <% if @hirings_transactions.present? %>
      <div class="featured-listings-box">
        <div class="sort_paginate_ajax" id="hirings">
          <%= render partial: "hirings", locals: {hirings: @hirings_transactions} %>
        </div>
      <% else%>
        <div class="featured-listings-box">
          <h1 class="featured-heading">Record not found.</h1>
        </div>
      <% end %>
    </div>
  </div>
</div>

<script type="text/javascript">
  $(function() {
    $(".sort_paginate_ajax th a, .sort_paginate_ajax .pagination a").on("click", function(){
      $.getScript(this.href);
      return false;
    });
  });
</script>