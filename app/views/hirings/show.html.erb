<table style="width: 100%">
  <tr>
    <td>
      <table style="width: 100%">
        <tr>
          <td class="UserLeftMenuGap">&nbsp;</td>
          <td style="width:85%">&nbsp;</td>
          <td>&nbsp;</td>
        </tr>
        <tr>
          <td valign="top">
          </td>
          <td class="PaddingRightBottom20px">
            <br />
            <div class="FloatRight PaddingRight10px">
              <table class="ButtonDarkGrey90pxHeight40px">
                <tr>
                  <td><a href="#">Print</a>
                  </td>
                </tr>
              </table>
            </div>
            <span class="OrderListingTitle">You're hiring Cristiano Ronaldo!</span>
            <br />
            Hiring ID:
            <%= @transaction.id %>
            <br /><br />
            <div class="divTable">
              <div class="divTableRow">
                <div class="divTableCell">
                  <div class="divTable OrderBox Width600Px FloatLeft">
                    <div class="divTableRow">
                      <div class="divTableCell Padding30Px FullWidth">
                        <div class="divTable">
                          <div class="divTableRow">
                            <div class="divTableCell VerticalAlignTop" style="width:25%">
                              <strong>Employee</strong>
                            </div>
                            <div class="divTableCell">
                              <div class="divTable">
                                <div class="divTableRow">
                                  <div class="divTableCell FloatLeft">
                                    <%= @listing.title %><br />
                                    <%= @listing.name %>
                                  </div>
                                  <div class="divTableCell FloatRight">
                                    <% if @listing.profile_picture.present? %>
                                      <%= image_tag @listing.profile_picture.url, class: 'Image150px' %>
                                    <% else %>
                                      <%= image_tag 'no-image.jpg', class: 'Image150px' %>
                                    <% end %>
                                  </div>
                                </div>
                              </div>
                            </div>
                          </div>
                        </div>
                        <br />
                        <div class="GreyLine1Px">
                        </div>
                        <br />
                        <div class="divTable">
                          <div class="divTableRow">
                            <div class="divTableCell VerticalAlignMiddle" style="width:25%">
                              <strong>Hiring Start</strong>
                            </div>
                            <div class="divTableCell VerticalAlignMiddle" style="width:25%">
                              <%= @transaction.start_date.strftime("%d %h %Y") %>
                            </div>
                            <div class="divTableCell VerticalAlignMiddle" style="width:25%">
                              <strong>Hiring End</strong>
                            </div>
                            <div class="divTableCell" style="width:25%">
                              <%= @transaction.end_date.strftime("%d %h %Y") %>
                            </div>
                          </div>
                        </div>
                        <br />
                        <div class="GreyLine1Px">
                        </div>
                        <br />
                        <div class="divTable">
                          <div class="divTableRow">
                            <div class="divTableCell VerticalAlignTop" style="width:25%">
                              <strong>Probation</strong>
                            </div>
                            <div class="divTableCell">
                              <%= @transaction.probationary_period.present? ? "#{@transaction.probationary_period} month" : "-" %>
                            </div>
                          </div>
                        </div>
                        <br />
                        <div class="GreyLine1Px">
                        </div>
                        <br />
                        <div class="divTable">
                          <div class="divTableRow">
                            <div class="divTableCell VerticalAlignTop" style="width:25%">
                              <strong>Timetable</strong>
                            </div>
                            <div class="divTableCell">
                              <div class="divTable">
                                <% @transaction.bookings.each do |booking| %>
                                  <div class="divTableRow">
                                    <div class="divTableCell VerticalAlignMiddle" style="width:33%">
                                      <%= booking.day.titleize %>
                                    </div>
                                    <div class="divTableCell VerticalAlignMiddle">
                                      <%= "#{booking.start_time.strftime("%H:%M")} - #{booking.end_time.strftime("%H:%M")}" %>
                                    </div>
                                  </div>
                                <% end %>
                              </div>
                            </div>
                          </div>
                        </div>
                        <br />
                        <div class="GreyLine1Px">
                        </div>
                        <br />
                        <div class="divTable">
                          <div class="divTableRow">
                            <div class="divTableCell VerticalAlignTop" style="width:25%">
                              <strong>Address</strong>
                            </div>
                            <div class="divTableCell">
                              <%= "#{@listing.address_1}" %><br />
                              <%= "#{@listing.city} #{@listing.state} #{@listing.post_code}" %><br />
                              <%= "#{@listing.country}" %>
                            </div>
                          </div>
                        </div>
                        <br />
                        <div class="GreyLine1Px">
                        </div>
                        <br />
                        <div class="divTable">
                          <div class="divTableRow">
                            <div class="divTableCell VerticalAlignTop" style="width:25%">
                              <strong>Billing</strong>
                            </div>
                            <div class="divTableCell">
                              <div class="divTable">
                                <div class="divTableRow">
                                  <div class="divTableCell">
                                    Weekly payment
                                  </div>
                                  <div class="divTableCell">
                                  </div>
                                </div>
                                <div class="divTableRow">
                                  <div class="divTableCell FloatLeft">
                                    $<%= "#{@listing.weekday_price} x #{@transaction.weekday_hours} hours" %>
                                  </div>
                                  <div class="divTableCell FloatRight">
                                    $<%= @listing.weekday_price * @transaction.weekday_hours %>
                                  </div>
                                </div>
                                <div class="divTableRow">
                                  <div class="divTableCell FloatLeft">
                                    $<%= "#{@listing.weekend_price} x #{@transaction.weekend_hours} hours (weekend)" %>
                                  </div>
                                  <div class="divTableCell FloatRight">
                                    $<%= @listing.weekend_price * @transaction.weekend_hours %>
                                  </div>
                                </div>
                                <div class="divTableRow">
                                  <div class="divTableCell FloatLeft">
                                    Tax withholding
                                  </div>
                                  <div class="divTableCell FloatRight">
                                    -$<%= @transaction.tax_withholding_amount %>
                                  </div>
                                </div>
                                <div class="divTableRow">
                                  <div class="divTableCell FloatLeft">
                                    Service fee
                                  </div>
                                  <div class="divTableCell FloatRight">
                                    $11.13
                                  </div>
                                </div>
                                <div class="divTableRow">
                                  <div class="divTableCell FloatLeft">
                                    <strong>Weekly 
                                    total</strong>
                                  </div>
                                  <div class="divTableCell FloatRight">
                                    <strong>$<%= @transaction.amount %></strong>
                                  </div>
                                </div>
                              </div>
                              <br />
                              <div class="GreyLine1Px">
                              </div>
                              <br />
                              Total employment contract
                              <div class="" id="TotalEmploymentContract">
                                <div class="divTable">
                                  <div class="divTableRow">
                                    <div class="divTableCell FloatLeft">
                                      $<%= "#{@listing.weekday_price} x #{@transaction.total_weekday_hours} hours" %>
                                    </div>
                                    <div class="divTableCell FloatRight">
                                      $<%= @listing.weekday_price * @transaction.total_weekday_hours %></div>
                                  </div>
                                  <div class="divTableRow">
                                    <div class="divTableCell FloatLeft">
                                      $<%= "#{@listing.weekend_price} x #{@transaction.total_weekend_hours} hours (weekend)" %>
                                    </div>
                                    <div class="divTableCell FloatRight">$<%= @listing.weekend_price * @transaction.total_weekend_hours %></div>
                                  </div>
                                  <div class="divTableRow">
                                    <div class="divTableCell FloatLeft">
                                      Tax withholding
                                    </div>
                                    <div class="divTableCell FloatRight">
                                      -$<%= @transaction.total_tax_withholding_amount %>
                                    </div>
                                  </div>
                                  <div class="divTableRow">
                                    <div class="divTableCell FloatLeft">
                                      Service fee
                                    </div>
                                    <div class="divTableCell FloatRight">
                                      $116.55
                                    </div>
                                  </div>
                                  <div class="divTableRow">
                                    <div class="divTableCell FloatLeft PaddingBottom10px">
                                      <strong>Total</strong>
                                    </div>
                                    <div class="divTableCell FloatRight PaddingBottom10px">
                                      <strong>$<%= @transaction.total_amount %></strong>
                                    </div>
                                  </div>
                                  <div class="divTableRow">
                                    <div class="divTableCell FloatLeft Text14px">
                                      <div class="GreyLine1Px">
                                      </div>
                                      <br />
                                      You will not pay the Total 
                                      employment contract upfront. You 
                                      chose to pay weekly.
                                    </div>
                                  </div>
                                </div>
                                <br />
                                <a href="javascript:;">Get receipt
                                </a>
                              </div>
                            </div>
                          </div>
                        </div>
                        <br />
                        <div class="GreyLine1Px">
                        </div>
                        <br />
                        <div class="divTable">
                          <div class="divTableRow">
                            <div class="divTableCell VerticalAlignTop" style="width:25%">
                              <strong>Need help?</strong>
                            </div>
                            <div class="divTableCell">
                              Visit the 
                              <a href="https://www.sparestaff.com.au/help" target="_blank">Help Center</a>.
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="divTableCell VerticalAlignTop PaddingLeft20px">
                  <div class="divTable OrderBox Width400Px">
                    <div class="divTableRow">
                      <div class="divTableCell Padding30Px FullWidth TextAllignCenter">
                        <%= image_tag "zidane-profile.jpg", class: "ProfileIcon100pxNoBorder" %><br />
                        <br />
                        <strong>Poster: <%= @listing.poster.name %></strong><br />
                        Have a question about your hiring? The 
                        best way to get information is to 
                        discuss with the Poster directly.<br />
                        <br />
                        <table class="ButtonOrange AlignCenter">
                          <tr>
                            <td>
                              <a href="javascript:;">Message Poster</a>
                            </td>
                          </tr>
                        </table>
                        <br />
                        <%= image_tag "tel-icon.jpg", class: "Height20px VerticalAlignMiddle" %>&nbsp;
                        <a href="javascript:;">+61 434 292 527</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </td>
          <td valign="top">&nbsp;
          </td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <td>&nbsp;
    </td>
  </tr>
  <tr>
    <td>
